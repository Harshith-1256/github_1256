import java.util.*;
public class string{
    public static boolean palindrome(String name){
        for(int i =0 ;i<name.length()/2;i++){
            if(charAt(i) != charAt(name.length() -1-i)){
                return false;
            }
        }
        return true;

    }
    public static void main(String args[]) {
        Scanner sc= new Scanner(System.in);
        String name = sc.nextLine();
        palindrome(name);
    }
}
